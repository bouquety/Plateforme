--------------------------- */\r\n\r\n\r\n\r\n/* ------------------------------------------------------------------ */\r\n/* HEADER */\r\n/* ------------------------------------------------------------------ */\r\n.navbar-light{\r\nwidth: 100%;\r\nbackground-color: rgba(191, 193, 191, 0.2);\r\n}\r\n\r\n.form-control {\r\nwidth: 350px;\r\n}\r\n\r\n.form-control i {\r\n  display: inline-block  \r\n}\r\n#resultat{\r\ncolor: #CC0033;\r\n}\r\n\r\n/* ------------------------------------------------------------------ */\r\n/*FORM                                                                */\r\n/* ------------------------------------------------------------------ */\r\n\r\nbody{\r\n  background-image: linear-gradient(\r\n    rgba(22, 22, 22, 0.8),\r\n    rgba(22, 22, 22, 0.8)\r\n\r\n    ),\r\n    url(img/fond.jpg);\r\n       background-size: cover;\r\n       background-position: center;\r\n       height: 100vh;\r\n       \r\n}\r\n.card-body{\r\n  background-color:rgba(173,173,173,\t 1)\r\n}\r\n\r\n/* ------------------------------------------------------------------ */\r\n/* MENU VERTICAL */\r\n/* ------------------------------------------------------------------ */\r\n\r\n\r\n.container{\r\n/*Création dimension du container qui contient la sidebar*/\r\nwidth: 100%; \r\nheight: 100%;\r\nmargin-left: -20px; /*Pour coller le menu à gauche de l'écran*/\r\n\r\n}\r\n\r\n.my-sidebar {\r\n/*Dimension de la sidebar*/\r\n\r\nposition: fixed; /*Fixer le menu vertical*/\r\nwidth: 160px; /*On définis la longueur */\r\n\r\nheight: 100%; /*Et la hauteur */\r\n\r\n background-color: rgba(191, 193, 191, 0.3); /*Couleur*/\r\n\r\nmargin: 0;\r\n\r\n}\r\n\r\n\r\n\r\n.my-sidebar a {\r\ndisplay: block; /*Afficher les éléments l'un en dessous de l'autre*/\r\n/*color: #626567*/\r\ncolor: black;\r\nmargin :0;\r\n\r\n\r\n}\r\n\r\n\r\n.toggle-btn {\r\nvisibility:hidden; /*Cacher la flèche par défault*/\r\n\r\n}\r\n\r\n\r\n\r\n.brand{\r\nfont-size: 14px; /*Taille du titre du menu vertical */\r\n\r\n}\r\n\r\n.nav-link{\r\npadding: 20px 25px; /*Ajoute de l'espace entre les éléments du menu*/\r\nmargin: 0;\r\n\r\n}\r\n\r\n.nav-link p {\r\nmargin : 0;\r\n\r\n  line-height: 31px; /*Espace entre les liens*/\r\n  font-size: 12px; /*Taille du texte*/\r\n  font-weight: 600;\r\n  text-transform: uppercase; /*Mets tous en majuscule*/\r\n  display: inline-flex; /*Affiche les éléments l'un après l'autre*/\r\n  border-bottom: solid black 2px; /*Ajoute un trait en dessous de chaque éléments*/\r\n  width: 100%;\r\n\r\n}\r\n\r\n.nav-link i {\r\n\r\n  margin: 0;\r\n  text-align: center; /*Centre l'image*/\r\n  vertical-align: middle;\r\n  width: 2em; /*Ajoute de l'espace entre l'image et le texte*/\r\n  \r\n\r\n  \r\n}\r\n\r\n.navbar-nav{\r\npadding: 0;\r\nmargin: 0;\r\n\r\n}\r\n\r\n/*.nav-link:hover,  apparence au survol du bouton \r\n.nav-link:active {\r\n apparence au clic \r\nbackground-color:  #777c81;\r\n}\r\n\r\n*/\r\n/* ------------------------------------------------------------------ */\r\n/* ------------------------------------------------------------------ */\r\n\r\n\r\n\r\n\r\n/* ------------------------------------------------------------------ */\r\n/* LEGENDE */\r\n/* ------------------------------------------------------------------ */\r\n\r\n.legende{\r\nwidth: 100%;\r\nheight: 100%;\r\n\r\n\r\n}\r\n.carreun{\r\nwidth:30px;\r\nheight:30px;\r\nbackground:#CC0033;\r\n}\r\n.carredeux{\r\nwidth:30px;\r\nheight:30px;\r\nbackground:#FF8C00;\r\n}\r\n.carretrois{\r\nwidth:30px;\r\nheight:30px;\r\nbackground:#808080;\r\n}  \r\n.navbar-brand {\r\nwidth:230px;\r\nmargin:0;\r\n}\r\n\r\n/* ------------------------------------------------------------------ */\r\n/* ------------------------------------------------------------------ */\r\n\r\n\r\n\r\n\r\n/* ------------------------------------------------------------------ */\r\n/* MEDIA */\r\n/* ------------------------------------------------------------------ */\r\n\r\n@media (max-width: 767px) {  /*A partir de 767 on fais : */\r\n.my-sidebar {\r\n  position: relative; /*Pour que le menu s'affiche bien entre le header et la carte*/\r\n  width: 100%;\r\n  display: block;\r\n}\r\n\r\n.menu-content{\r\ndisplay: block; /*Afficher les éléments du menu l'un en dessous de l'autre*/\r\n}\r\n\r\n.container{\r\n  width: 100%;\r\n\r\n}\r\n\r\n.toggle-btn {\r\n  visibility: visible; /*On affiche la flèche*/ \r\n  cursor: pointer;  /*Mets la souris en mode pointer*/ \r\n  position: absolute; /*Aligne la flèche et le titre du menu*/\r\n  right: 10px; /*Positionnement de la flèche*/\r\n  top: 25px;/*Hauteur de la flèche*/\r\n  color: #000;\r\n  width: 40px;\r\n  text-align: center;\r\n}\r\n.brand {\r\n  text-align: left !important;  /*Positionner le texte à gauche*/\r\n  font-size: 14px; /*Taille du texte*/\r\n  line-height: 50px !important; /*Hauteur du menu*/\r\n}\r\n}\r\n\r\n\r\n\r\n@media (min-width: 767px) { /*Jusqu'a 767 px on fais : */\r\n\r\n\r\n\r\n\r\n.content{\r\n\r\n  left: 0;\r\n  margin-left: 160px; /*Positionnement de la carte et de la légende*/\r\n  min-height: 100%;\r\n  \r\n\r\n}\r\n\r\n.collapse:not(.show) {\r\n  display: block!important ; /*On oblige l'affichage du contenu du menu */\r\n}\r\n\r\n}\r\n@media (min-width: 576px){\r\n.container {\r\n   max-width: 900px!important; \r\n}\r\n}\r\n/* ------------------------------------------------------------------ */\r\n/* ------------------------------------------------------------------ */\r\n\r\n"},"lineCount":null}},"error":null,"hash":"04bc6ebe8eae0bf1188d7c70bfd1f43a","cacheData":{"env":{}}}