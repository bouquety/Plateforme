* ------------------------------------------------------------------ */\n.test{\nbackground-color: #eceff1 ;\n\n}\n\n/* ------------------------------------------------------------------ */\n/* HEADER */\n/* ------------------------------------------------------------------ */\n.navbar-light{\nwidth: 100%;\nbox-shadow: 1px 1px 5px #555;\nheight: 45px;\n\n}\n.navbar-nav i {\nwidth: 1em;\n}\n.nav.nav-tabs {\n  float: left;\n  display: block;\n  margin-right: 10px;\n  border-bottom: 0;\n  padding-right: 15px;\n}\n\n/* ------------------------------------------------------------------ */\n/*FORM                                                                */\n/* ------------------------------------------------------------------ */\n.map{\nheight: 100vh;\nwidth: 100%;\n}\n/* ------------------------------------------------------------------ */\n/* MENU VERTICAL */\n/* ------------------------------------------------------------------ */\n.container{\n/*Création dimension du container qui contient la sidebar*/\nwidth: 100%; \nheight: 100%;\nmargin-left: -15px; /*Pour coller le menu à gauche de l'écran*/\n}\n\n.my-sidebar {\n/*Dimension de la sidebar*/\nposition: fixed; /*Fixer le menu vertical*/\nwidth: 51px; /*On définis la longueur */\nheight: 100%; /*Et la hauteur */\nbackground-color: #88a2bb;\npadding: 0;\nborder-radius: 5px;\nmargin: 0;\nmargin-top : 10px;\n}\n.my-sidebar a {\n /*Afficher les éléments l'un en dessous de l'autre*/\n/*color: #626567*/\ncolor: rgb(255, 255, 255);\n}\n.navbar-nav{\npadding: 0;\nmargin: 0;\n}\n\n\n/* ------------------------------------------------------------------ */\n/* ------------------------------------------------------------------ */\n.nav-tabs .nav-link.active {\n  background-color: rgba(59, 59, 59, 0.158);\n}\n  .nav-tabs .nav-link:hover{\n    background-color: rgba(59, 59, 59, 0.158);\n    }\n    .nav-tabs .nav-link{\n      margin : 0;\n      width: 100%;\n      line-height: 40px;\n      border-top : 3px solid #f8f9fa\n      } \n      \n\n    \n\n/* ------------------------------------------------------------------ */\n/* LEGENDE */\n/* ------------------------------------------------------------------ */\n\n.legende{\nwidth: 18%;\nposition: absolute;\ntop: 175px;\nleft: 1540px;\npadding: 5px;\nbackground-color: #ffffff;\nborder-radius: 5px;\nvisibility: hidden;\n}\n.carreun{\nwidth:30px;\nheight:30px;\nbackground:#CC0033;\n}\n.carredeux{\nwidth:30px;\nheight:30px;\nbackground:#FF8C00;\n}\n.carretrois{\nwidth:30px;\nheight:30px;\nbackground:#808080;\n}  \n.navbar-brand {\nwidth:230px;\nmargin:0;\n}\n\n/* ------------------------------------------------------------------ */\n/* ------------------------------------------------------------------ */\n.container-fluid{\nbackground-color: #eceff1 ;\nheight: 100%;\n\n}\n/* ------------------------------------------------------------------ */\n/* MEDIA */\n/* ------------------------------------------------------------------ */\n\n\n/* ------------------------------------------------------------------ */\n/* MEDIA */\n/* ------------------------------------------------------------------ */\n\n@media (max-width: 767px) {  /*A partir de 767 on fais : */\n.my-sidebar {\n  position: relative; /*Pour que le menu s'affiche bien entre le header et la carte*/\n  width: 100%;\n\n}\n\n\n.menu-content{\ndisplay: block; /*Afficher les éléments du menu l'un en dessous de l'autre*/\n}\n\n.container{\n  width: 100%;\n\n}\n#hamburger{\n  color: rgb(33, 67, 90);\n}\n.my-sidebar #logo{\n  display: none;\n}\n\n.brand {\n  text-align: left !important;  /*Positionner le texte à gauche*/\n  font-size: 14px; /*Taille du texte*/\n  line-height: 50px !important; /*Hauteur du menu*/\n}\n}\n\n\n\n@media (min-width: 767px) { /*Jusqu'a 767 px on fais : */\n#hamburger{\n  display: none;\n}\n\n.content{\n\n  left: 0;\n  margin-left: 39px; /*Positionnement de la carte et de la légende*/\n  min-height: 100%;\n  margin-top : 10px;\n}\n\n.collapse:not(.show) {\n  display: block!important ; /*On oblige l'affichage du contenu du menu */\n}\n\n}\n@media (min-width: 576px){\n.container {\n   max-width: 900px!important; \n}\n}\n/* ------------------------------------------------------------------ */\n/* ------------------------------------------------------------------ */\n/* ------------------------------------------------------------------ */\n/* ------------------------------------------------------------------ */\n\n"},"lineCount":205}},"error":null,"hash":"44c5034776131ec5ab4655ff336b8895","cacheData":{"env":{}}}